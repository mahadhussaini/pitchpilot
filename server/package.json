{
  "name": "pitchpilot",
  "version": "1.0.0",
  "description": "AI-powered pitch deck and fundraising assistant for startups",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node index.js"
  },
  "keywords": ["pitch-deck", "startup", "ai", "fundraising", "saas"],
  "author": "PitchPilot Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.20.1",
    "langchain": "^0.0.200",
    "socket.io": "^4.7.4",
    "nodemailer": "^6.9.7",
    "stripe": "^14.7.0",
    "uuid": "^9.0.1",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  }
} 